{
  "name": "basic-task",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "build": "npm run build:client",
    "build:client": "vite build --outDir dist",
    "build:server": "tsc src/server/**/*.ts --outDir dist-server",
    "start": "node .modelence/build/app.mjs",
    "dev": "modelence dev",
    "dev:frontend": "modelence dev",
    "dev:backend": "tsx watch src/server/simple-server.ts",
    "dev:both": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "preview": "vite preview --outDir dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-validator": "^7.2.1",
    "modelence": "^0.5.6",
    "mysql2": "^3.14.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.22.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/sqlite3": "^3.1.11",
    "@vitejs/plugin-react": "^5.0.1",
    "autoprefixer": "^10.4.17",
    "concurrently": "^9.2.1",
